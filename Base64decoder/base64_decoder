#!/usr/bin/python
#https://chousensha.github.io/blog/2015/02/09/pentest-lab-flick/
#Check the base64 decoder
import base64

secret = ''

count = 0
while True:
    try:
        secret = base64.b64decode(secret)
        print 'Try ' + str(count) + ': ' + secret
        count += 1
    except TypeError:
        print 'Done'
        break
